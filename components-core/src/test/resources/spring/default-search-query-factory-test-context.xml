<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="ch.alv.components.core.spring.internal.DefaultApplicationContextProvider" />

    <bean class="ch.alv.components.core.search.internal.DefaultSearchQueryFactory" />

    <bean class="ch.alv.components.core.mock.MockValuesProvider" />
    <bean id="search" class="ch.alv.components.core.mock.MockSearch">
        <constructor-arg index="0" value="testSearch" />
    </bean>
    <bean id="searchCloneOne" class="ch.alv.components.core.mock.MockSearch">
        <constructor-arg index="0" value="testSearchClones" />
    </bean>
    <bean id="searchCloneTwo" class="ch.alv.components.core.mock.MockSearch">
        <constructor-arg index="0" value="testSearchClones" />
    </bean>
</beans>


