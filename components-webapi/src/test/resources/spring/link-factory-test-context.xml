<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <bean class="ch.alv.components.web.mapper.LinkFactory" />

    <bean id="contextProvider" class="ch.alv.components.core.spring.DefaultApplicationContextProvider" />

    <bean class="ch.alv.components.web.endpoint.EndpointRegistry">
        <constructor-arg index="0" ref="contextProvider" />
    </bean>

    <bean name="mapperFactoryModule.mapperFactoryStor.service" class="ch.alv.components.web.mock.MockDataService" />

    <bean id="endpoint" class="ch.alv.components.web.mock.MockEndpoint" />

    <bean class="ch.alv.components.web.context.DefaultServletRequestProvider" />

    <bean class="org.springframework.mock.web.MockHttpServletRequest" />

    <context:annotation-config />

</beans>


